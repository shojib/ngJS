.default-container

  ul.nav.nav-tabs(role='tablist')
    li(role='presentation')
      a(href='#/speedtest/angularjs', aria-controls='angularjs', role='tab', data-toggle='tab') AngularJS
    li(role='presentation', class="active")
      a(href='#/speedtest/reactjs', aria-controls='reactjs', role='tab', data-toggle='tab') ReactJS
      
  br
  .tab-content
      button.btn.btn-primary(ng-click="startTimer()") Start 
      |  
      button.btn.btn-danger(ng-click="stopTimer()") Stop
      
      br
      br
      div.speed-table#table1
      div.speed-table#table2
      div.speed-table#table3
      div.speed-table#table4
      div.speed-table#table5