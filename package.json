{
  "name": "ngJS",
  "version": "0.1.0",
  "main": "ngjs.js",
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "scripts": {
    "build": "browserify ./src/modules/ngjs.js -o ./public/modules/ngjs.min.js -dv",
    "watch": "watchify ./src/modules/ngjs.js -o ./public/modules/ngjs.min.js -dv",
    "start": "npm run build && npm run watch"
  },
  "dependencies": {
    "almond": "0.3.1",
    "angular": "1.4.7",
    "angular-animate": "1.4.7",
    "angular-cookies": "1.4.7",
    "angular-resource": "1.4.7",
    "angular-sanitize": "1.4.7",
    "angular-translate": "2.8.1",
    "angular-translate-loader-static-files": "2.8.1",
    "angular-translate-loader-url": "2.8.1",
    "angular-translate-storage-cookie": "2.8.1",
    "angular-translate-storage-local": "2.8.1",
    "angular-ui-router": "0.2.15",
    "bootstrap": "3.3.5",
    "react": "~0.14.6",
    "react-addons": "^0.9.0",
    "react-dom": "^0.14.7",
    "requirejs": "2.1.20"
  },
  "devDependencies": {
    "angular-mocks": "1.4.7",
    "grunt": "~0.4.4",
    "grunt-browserify": "^4.0.1",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-compass": "^0.7.2",
    "grunt-contrib-connect": "^0.7.1",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-jasmine": "^0.6.5",
    "grunt-contrib-requirejs": "~0.4.3",
    "grunt-contrib-uglify": "^0.4.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-karma": "^0.8.3",
    "grunt-protractor-runner": "^0.2.5",
    "grunt-react": "^0.12.3",
    "grunt-selenium-webdriver": "~0.2.392",
    "karma": "^0.12.32",
    "karma-coverage": "~0.2.1",
    "karma-jasmine": "^0.1.5",
    "karma-phantomjs-launcher": "~0.1.2",
    "karma-requirejs": "~0.2.1",
    "react": "^0.14.6",
    "react-addons-pure-render-mixin": "^0.14.7"
  },
  "keywords": [
    "javascript",
    "react",
    "react.js",
    "react-component",
    "angular",
    "angular.js",
    "rendering",
    "performance",
    "tests",
    "performance tests"
  ],
  "folders": {
    "build": "public/",
    "src": "src/",
    "entity_src": "src/modules/",
    "entity_build": "public/modules/",
    "libs": "src/libs/",
    "test": "test/"
  }
}
